    <label>Element</label>
    <description>Temp</description>
    <graphicData>
      <texPath>Things/Item/Element</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
